{
  "name": "@fluencelabs/marine-js",
  "description": "Marine-js",
  "version": "0.7.4",
  "exports": {
    ".": "./dist/index.js",
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./MarineService": {
      "types": "./dist/MarineService.d.ts",
      "import": "./dist/MarineService.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./marine-js.wasm": "./dist/marine-js.wasm",
    "./dist/types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./dist/MarineService": {
      "types": "./dist/MarineService.d.ts",
      "import": "./dist/MarineService.js"
    },
    "./dist/config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./dist/marine-js.wasm": "./dist/marine-js.wasm"
  },
  "typesVersions": {
    "*": {
      "types.d.ts": [
        "./dist/types.d.ts"
      ],
      "MarineService.d.ts": [
        "./dist/MarineService.d.ts"
      ],
      "config.d.ts": [
        "./dist/config.d.ts"
      ]
    }
  },
  "type": "module",
  "repository": "https://github.com/fluencelabs/marine",
  "author": "Fluence Labs",
  "license": "Apache 2.0",
  "files": [
    "dist/*",
    "!lib/__test__/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "node update-imports.cjs && rm -rf ./src/snippets && cp -R ../marine-js-pkg/snippets ./src/",
    "postbuild": "cp ../marine-js-pkg/marine_js_bg.wasm ./dist/marine-js.wasm ",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "private": false,
  "dependencies": {
    "@wasmer/wasi": "0.12.0",
    "@wasmer/wasmfs": "0.12.0",
    "default-import": "1.1.5"
  },
  "devDependencies": {
    "@babel/parser": "7.22.7",
    "@babel/traverse": "7.22.8",
    "@fluencelabs/avm": "0.54.0",
    "@jest/globals": "29.6.2",
    "@types/download": "8.0.1",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.59",
    "download": "8.0.0",
    "jest": "29.4.1",
    "recast": "0.23.1",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "typescript": "4.8.3"
  }
}
